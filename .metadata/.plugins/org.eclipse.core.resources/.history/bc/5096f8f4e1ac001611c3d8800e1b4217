import java.math.*;

public class Main {
	static BigInteger a = BigInteger.valueOf(1);

	public static void main(String[] args) {

		int currentNumber, dividers;
		for (currentNumber = 1; currentNumber < 1000; currentNumber++) {
			dividers = 0;
			for (int i = 1; i <= currentNumber; i++) {
				if (currentNumber % i == 0)
					dividers++;
			}
			if (dividers <= 2) {
				a = a.multiply(BigInteger.valueOf(currentNumber));
				// System.out.println(currentNumber);
			}
		}
		a = a.add(BigInteger.valueOf(1103));
		System.out.println("1511");
		System.out.println(a.toString());

		// a = BigInteger.valueOf(129419);
		boolean simple = true;
		BigInteger i = BigInteger.valueOf(2);
		for (i = BigInteger.valueOf(2); !(i.equals(a)); i = i.add(BigInteger.ONE)) {
			if (a.mod(i) == BigInteger.ZERO) {
				simple = false;
				System.out.println(i.toString() + "*" + a.divide(i).toString());
				break;
			}
		}

		if (simple) {
			System.out.println("This number is a simple!");
		} else {
			System.out.println("This number is a not simple!");
		}
	}

}