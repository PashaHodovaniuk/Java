public abstract class MenuRealize {
    protected MenuRealize menu;
    /* run our menu*/
   
    public MenuRealize() {
        menu = this;
    }
        
    public MenuRealize runMenu() {
        printText();
        handleMenu();
        return menu;
    }
    
    /* print text message*/
    public abstract void printText();
    
    /* handle click*/
    public abstract void handleMenu();

}