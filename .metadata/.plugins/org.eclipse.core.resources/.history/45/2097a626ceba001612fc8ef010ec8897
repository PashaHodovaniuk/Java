public class Menu extends MenuRealize {

	public void Select() {
		InOut.Outln("1 - Кофеварка");
		InOut.Outln("2 - Духовка");
		InOut.Outln("3 - Кондиционер");
		InOut.Outln("4 - Вытяжка");
		InOut.Outln("5 - Стиральная машинка");
	}

	public void runMenu() {
		
		Select();		
		String selected;
		selected = InOut.In();

		switch (selected) {
		case "1":
			menuCoffeeMaker();
			break;
		case "2":
			menuOver();
			break;
		case "3":
			menuAirConditioning();
			break;
		case "4":
			menuCookerHood();
			break;
		case "5":
			menuWishingMachine();
			break;
		default:
			break;
		}
	}
}
